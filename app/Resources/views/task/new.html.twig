{% extends 'base.html.twig' %}

{% block body %}
  <h1>Task creation</h1>

  {{ form_start(form) }}
    {# form_widget(form) #}
    {{ form_errors(form) }}
    {{ form_row(form.task) }}
    <div class="hidden">{{ form_widget(form.complete) }}</div>
    <input type="submit" value="Create" />
  {{ form_end(form) }}

  <ul>
    <li>
      <a href="{{ path('task_index') }}">Back to the list</a>
    </li>
  </ul>
{% endblock %}
